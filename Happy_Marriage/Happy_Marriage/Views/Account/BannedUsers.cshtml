@model User
@using System.Text.Json
@{
    string jsonusers = TempData["users"].ToString();
    List<User> users = new List<User>();
    if (jsonusers != null)
    {
        users = JsonSerializer.Deserialize<List<User>>(jsonusers);
    }
}
<div class="container">
    @foreach (var user in users)
    {
        <div class="flex-row">
            <form>
                <input id="userid" name="userid" value=@user.UserId />

                <div class="col">@user.UserName</div>
                <div class="col">@user.ApprovalStatus</div>
                <div class="col">
                    <button formaction="/ProfilePage/OthersProfile/" formmethod="post">Visit Profile</button>
                </div>
                <div class="col">
                    <button formaction="/Account/UpliftBan/" formmethod="post">Uplift Ban</button>
                </div>


            </form>
        </div>
    }
</div>

